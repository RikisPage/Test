  "engines": {
    "firefox": ">=38.0a1"
  },
  "license": "MIT",
  "permissions": {
    "private-browsing": true
  },
  "preferences": [
    {
      "name": "onDuplicateTabDetected",
      "title": "On duplicate tab detected",
      "type": "radio",
      "hidden": true,
      "options": [
        {
          "label": "doNothing",
          "value": "N"
        },
        {
          "label": "showPanel",
          "value": "S"
        },
        {
          "label": "autoCloseTab",
          "value": "A"
        }
      ],
      "value": "S"
    },
    {
      "name": "keepTabBasedOnAge",
      "title": "Automatic close priority - Based on age",
      "type": "radio",
      "hidden": true,
      "options": [
        {
          "label": "keepNewerTab",
          "value": "N"
        },
        {
          "label": "keepOlderTab",
          "value": "O"
        }
      ],
      "value": "O"
    },
    {
      "name": "keepTabWithHistory",
      "title": "Automatic close priority - Keep tab with Back/Forward history",
      "type": "bool",
      "hidden": true,
      "value": true
    },
    {
      "name": "onRemainingTab",
      "title": "After automatic close - On remaining tab",
      "type": "radio",
      "hidden": true,
      "options": [
        {
          "label": "doNothing",
          "value": "N"
        },
        {
          "label": "activateTab",
          "value": "A"
        },
        {
          "label": "applyNewTabBehaviourOnTab",
          "value": "B"
        }
      ],
      "value": "A"
    },
    {
      "name": "ignoreHashPart",
      "title": "Ignore hash part in url",
      "type": "bool",
      "hidden": true,
      "value": false
    },
    {
      "name": "ignoreSearchPart",
      "title": "Ignore search part in url",
      "type": "bool",
      "hidden": true,
      "value": false
    },
    {
      "name": "ignorePathPart",
      "title": "Ignore path part in url",
      "type": "bool",
      "hidden": true,
      "value": false
    },
    {
      "name": "ignoreTabsFromOtherGroups",
      "title": "Ignore tabs from other groups",
      "type": "bool",
      "hidden": true,
      "value": true
    },
    {
      "name": "duplicateTabBadgeColor",
      "title": "Duplicate tabs button color",
      "type": "string",
      "value": "255, 0, 0"
    },
    {
      "name": "noDuplicateTabBadgeColor",
      "title": "No duplicate tab button color",
      "type": "string",
      "value": "30, 144, 255"
    }
  ]
}
